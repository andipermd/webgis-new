    "import requests\n",
    "import time\n",
    "\n",
    "url = \"http://localhost:3000/areas\"\n",
    "\n",
    "while True:\n",
    "    response = requests.get(url)\n",
    "\n",
    "    if response.status_code == 200:\n",
    "        data = response.json()\n",
    "        dataLen = len(data)\n",
    "\n",
    "        if dataLen > 0:\n",
    "            print(\"✅ Data ditemukan, menjalankan kode...\")\n",
    "            # 🟢 Tambahkan kode yang ingin dijalankan di sini\n",
    "            break  # Hentikan loop setelah data ditemukan\n",
    "        else:\n",
    "            print(\"⏳ Data masih kosong, mencoba lagi...\")\n",
    "\n",
    "    else:\n",
    "        print(f\"❌ Error: {response.status_code}\")\n",
    "\n",
    "    time.sleep(5)  # Tunggu 5 detik sebelum mencoba lagi\n"
   ]
